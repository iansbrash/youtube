{
  "name": "next-auth-multi-tenancy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "check-types": "NODE_OPTIONS='--max-old-space-size=8192' tsc --pretty --noEmit",
    "lint-fix": "npx eslint --fix .",
    "build": "next build",
